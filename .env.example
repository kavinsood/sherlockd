# Environment variables for sherlockd
# Copy this file to .env and fill in your actual values

# Backend API URL - required for the worker
# This is the URL of your backend service that processes the URLs
KITSUNE_API_URL=https://hostedbackend.com/analyze

# Frontend API URL - optional, has default
# This is used by the frontend for configuration (optional since it has a default)
VITE_RENDER_API_URL=https://hostedbackend.com/analyze

# Turnstile configuration (optional)
# Get these from Cloudflare Turnstile dashboard
WEB_TURNSTILE_SITE_KEY=your_turnstile_site_key_here
WEB_TURNSTILE_SECRET_KEY=your_turnstile_secret_key_here
